    </class>
    <class name="team_f.database_wrapper.database.WoodInstrumentationEntity" table="WoodInstrumentation" schema="sem4_team2">
        <id name="woodInstrumentationId">
            <column name="woodInstrumentationID" sql-type="int(11)"/>
        </id>
        <property name="flute">
            <column name="flute" sql-type="int(11)"/>
        </property>
        <property name="oboe">
            <column name="oboe" sql-type="int(11)"/>
        </property>
        <property name="clarinet">
            <column name="clarinet" sql-type="int(11)"/>
        </property>
        <property name="bassoon">
