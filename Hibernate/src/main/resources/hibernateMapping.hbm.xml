        </property>
        <property name="violin2">
            <column name="violin2" sql-type="int(11)"/>
        </property>
        <property name="viola">
            <column name="viola" sql-type="int(11)"/>
        </property>
        <property name="violincello">
            <column name="violincello" sql-type="int(11)"/>
        </property>
        <property name="doublebass">
            <column name="doublebass" sql-type="int(11)"/>
